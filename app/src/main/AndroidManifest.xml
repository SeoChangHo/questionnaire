<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zzango.questionnaire">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/questionnaire_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/questionnaire_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OralExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LocalList.ListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CommonExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ServerListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MentalExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ElderlyExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CognitiveExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SmokingExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ExerciseExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DrinkingExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CancerExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".NutritionExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SettingActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".LocalList.ListDetailActivity"></activity>
    </application>

</manifest>