<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fineinsight.zzango.questionnaire">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/questionnaire_logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/questionnaire_logo2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.fineinsight.zzango.questionnaire.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fineinsight.zzango.questionnaire.OralExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.LocalList.ListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.CommonExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.ServerListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.MentalExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.ElderlyExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.CognitiveExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.SmokingExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.ExerciseExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.DrinkingExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.CancerExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.NutritionExaminationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fineinsight.zzango.questionnaire.SettingActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.fineinsight.zzango.questionnaire.LocalList.ListDetailActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>